{% extends "index.html.twig" %}

 {% block title %}Contatos{% endblock %}

  {% block teste %}
<div class="col-md-6 offset-md-3 ">
								<div class="contact-form ">
										<h1 class="py-2 text-center">{{Titulo}}</h1>
											<h4 class="text-success py-4">{{msg}}<h4>

										{{ form_start(form) }}
											<div class="row ">
												{{ form_errors(form) }}

												<div class="col-md-6 col-xs-4 py-2">
												{{ form_row(form.nome) }}
												</div>
												<div class="col-md-6 col-xs-4 py-2">
												{{ form_row(form.sobrenome) }}
												</div>
												{{ form_row(form.email) }}
												<div class="col-md-6 col-xs-4 py-2">
												{{ form_row(form.data) }}
												</div>
												<div class="col-md-6 col-xs-4 py-2" >
												{{ form_row(form.telefone) }}
												</div>
												{{ form_row(form.mensagem) }}
											</div>
									
										{{ form_end(form) }}
								
							</div>
			      			</div>
 {% endblock %}